{% extends 'layout.html.twig' %}

{% block title %}IntegrationStep index{% endblock %}

{% block body %}
    <h1>IntegrationStep index</h1>

    <table class="table text-white">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Description</th>
                <th>Duration</th>
                <th>FontAwesome</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for integration_step in integration_steps %}
            <tr>
                <td>{{ integration_step.id }}</td>
                <td>{{ integration_step.name }}</td>
                <td>{{ integration_step.description }}</td>
                <td>{{ integration_step.duration }}</td>
                <td>{{ integration_step.fontAwesome }}</td>
                <td>
                    <a href="{{ path('integration_step_show', {'id': integration_step.id}) }}">show</a>
                    <a href="{{ path('integration_step_edit', {'id': integration_step.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('integration_step_new') }}">Create new</a>
{% endblock %}
