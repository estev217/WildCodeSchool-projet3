{% extends 'base.html.twig' %}

{% block main %}
<div class="container-fluid container-login min-vh-100">
    <div class="login">
    <img src="{{asset('build/images/logo.png')}}" class="logo" alt="logo">
        {{ form_start(form) }}
            {{ form_row(form.email) }}
            {{ form_row(form.password, {'label' : 'Mot de passe'}) }}
        <button class="btn btn-primary mt-4">{{ button_label|default('Me connecter') }}</button>
        {{ form_end(form, {'render_rest': false}) }}
    </div>
</div>
{% endblock %}

